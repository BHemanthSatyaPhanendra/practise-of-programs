#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int n;
    scanf("%d",&n);
    int arr[n];
    int i;
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    int s=0;
    for(i=0;i<n;i++)
    {
        s=s+arr[i];
    }
    printf("%d",s);
    return 0;
}


Sample Input 0

6
16 13 7 2 1 12 
Sample Output 0

51
